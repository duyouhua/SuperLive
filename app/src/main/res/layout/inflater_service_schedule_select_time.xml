<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >
    
    <RelativeLayout 
        android:layout_width="match_parent"
        android:layout_height="match_parent"   
        android:padding="10dp"  >

     <FrameLayout
         android:layout_width="match_parent"
         android:layout_height="300dp"
         android:background="@drawable/retangle_soild_withe" >

         <RelativeLayout
             android:layout_width="match_parent"
             android:layout_height="215dp"
             android:layout_marginTop="40dp"
             android:background="@color/white"
             android:gravity="center_vertical" >

             <TextView
                 android:id="@+id/textView5"
                 android:layout_width="wrap_content"
                 android:layout_height="wrap_content"
                 android:layout_alignBaseline="@+id/textView4"
                 android:layout_alignBottom="@+id/textView4"
                 android:layout_alignRight="@+id/inflater_service_schedule_select_hour"
                 android:layout_marginRight="14dp"
                 android:text="时"
                 android:textColor="@color/grey" />

             <com.joytouch.superlive.widget.TimeSelectDialog.TimeWheelView
                 android:id="@+id/inflater_service_schedule_select_hour"
                 android:layout_width="70dp"
                 android:layout_height="fill_parent"
                 android:layout_alignParentTop="true"
                 android:layout_toRightOf="@+id/inflater_service__schedule_select_date" >
             </com.joytouch.superlive.widget.TimeSelectDialog.TimeWheelView>

             <TextView
                 android:id="@+id/textView4"
                 android:layout_width="wrap_content"
                 android:layout_height="wrap_content"
                 android:layout_alignParentRight="true"
                 android:layout_centerVertical="true"
                 android:text="分"
                 android:textColor="@color/black"
                 android:visibility="gone" />

             <com.joytouch.superlive.widget.TimeSelectDialog.TimeWheelView
                 android:id="@+id/inflater_service_schedule_select_minute"
                 android:layout_width="70dp"
                 android:layout_height="fill_parent"
                 android:layout_alignParentRight="true"
                 android:layout_alignParentTop="true"
                 android:layout_toRightOf="@+id/textView5" >

             </com.joytouch.superlive.widget.TimeSelectDialog.TimeWheelView>
    
             <TextView
                 android:id="@+id/textView7"
                 android:layout_width="wrap_content"
                 android:layout_height="wrap_content"
                 android:layout_alignBaseline="@+id/textView6"
                 android:layout_alignBottom="@+id/textView6"
                 android:layout_alignRight="@+id/inflater_service_schedule_select_hour"
                 android:text="时"
                 android:textColor="@color/black" />

             <TextView
                 android:id="@+id/textView6"
                 android:layout_width="wrap_content"
                 android:layout_height="wrap_content"
                 android:layout_alignParentRight="true"
                 android:layout_centerVertical="true"
                 android:layout_marginRight="16dp"
                 android:text="分"
                 android:textColor="@color/black" />

             <com.joytouch.superlive.widget.TimeSelectDialog.TimeWheelView
                 android:id="@+id/inflater_service__schedule_select_date"
                 android:layout_width="130dp"
                 android:layout_height="fill_parent"
                 android:layout_alignParentLeft="true"
                 android:layout_alignParentTop="true" >
             </com.joytouch.superlive.widget.TimeSelectDialog.TimeWheelView>

         </RelativeLayout>

         <TextView
             android:textColor="#666666"
             android:id="@+id/id_show_time_select"
             android:layout_width="wrap_content"
             android:layout_height="wrap_content"
             android:layout_gravity="center_horizontal"
             android:layout_marginTop="5dp"
             android:text="设置提醒时间"
             android:textSize="17sp" />

         <TextView
             android:visibility="gone"
             android:id="@+id/id_service_showdate_time_tv"
             android:layout_width="wrap_content"
             android:layout_height="wrap_content"
             android:layout_gravity="center_horizontal"
             android:layout_marginTop="45dp"
             android:textColor="#a383f0" />


      <LinearLayout
             android:layout_width="match_parent"
             android:layout_height="35dp"
             android:layout_marginTop="259dp"
             android:orientation="horizontal"
             android:paddingLeft="10dp"
             android:paddingRight="10dp">


             <RelativeLayout
                 android:id="@+id/id_dialog_service_schedule_positive"
                 android:layout_width="wrap_content"
                 android:layout_height="match_parent"
                 android:layout_weight="0.8"
                 android:background="@drawable/retang_theme"
                >

                 <TextView
                     android:id="@+id/textView3"
                     android:layout_width="wrap_content"
                     android:layout_height="wrap_content"
                     android:layout_centerHorizontal="true"
                     android:layout_centerVertical="true"
                     android:text="确定"
                     android:textColor="@color/white"
                     android:textSize="17sp" />
             </RelativeLayout>
          <View
              android:layout_width="20dp"
              android:layout_height="42dp"
              android:background="@color/white"/>
          <RelativeLayout
              android:id="@+id/id_dialog_service_schedule_negtive"
              android:layout_width="wrap_content"
              android:layout_height="35dp"
              android:layout_weight="0.75"
              android:background="@drawable/retangle_gray" >
              <TextView
                  android:id="@+id/textView2"
                  android:layout_width="wrap_content"
                  android:layout_height="wrap_content"
                  android:layout_centerHorizontal="true"
                  android:layout_centerVertical="true"
                  android:text="取消"
                  android:textColor="@color/white"
                  android:textSize="17sp" />
          </RelativeLayout>
         </LinearLayout>
     </FrameLayout>
    </RelativeLayout>
</LinearLayout>
